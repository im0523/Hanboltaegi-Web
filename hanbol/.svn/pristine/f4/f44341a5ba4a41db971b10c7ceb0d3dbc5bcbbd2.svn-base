@charset "UTF-8";

h2 { text-align: center;}
a:hover {cursor: pointer;font-weight: bold;}

.input_search{ width: 500px; height: 30px; }

body { 
	margin: 0 auto;
	text-align: center;
}
table {
	margin: 0 auto;
	width: 80%;
	border-collapse: collapse;
}
table a:hover{ 
	cursor: pointer;
	font-weight: bold;  
}
.btn-fill, .btn-empty{
	text-align: center;
	padding: 3px 10px;
	border: 1px solid #34a2d;
	border-radius: 10px;
	box-shadow: 2px 2px 3px #356628;
}
.btn-fill{
	background-color: #74b243;
	color: #fff;
}
.btn-empty{
	background-color: #fff;
	color: #398a24;
}

.btn-fill-s, .btn-empty-s{
	text-align: center;
	padding: 2px 6px;
	border: 1px solid #78aeff;
	border-radius: 3px;
	box-shadow: 2px 2px 3px #022d72;
	font-size: 13px;
}
.btn-fill-s{
	background-color: #78aeff;
	color: #fff;
}
.btn-empty-s{
	background-color: #fff;
	color: #78aeff;
}

.left { text-align: left; }
.center { text-align: center; }
.right { text-align: right; }

input[type=text], input[type=password] {
	height: 22px;
	padding-left: 5px; padding-right: 5px;
}

.btn-img {
	width:22px;
	vertical-align: middle;
	cursor: pointer;
}

#list-top {
	padding-left: 10%;
    padding-bottom: 30px;
}
#list-top select[name=search]{
	height: 28px; width: 82px;
}
#list-top input[name=keyword]{
	width: 300px;
	vertical-align: top;
}

.btn-joongbok{
	text-align: center;
	padding: 2px 6px;
	border: 1px solid;
	border-radius: 3px;
	box-shadow: 2px 2px 3px;
	font-size: 13px;
	color: white;
	background-color: #74b243;
	height: 20px;
	line-height: 20px;
}

.cancel {
	width: 200px;
	height: 60px;
	text-align: center;
	line-height: 60px;
	background: #8e8e8e;
	color: white;
	cursor: pointer;
	font-weight: bold; 
}

img {  object-fit: contain; }


.regi_content { width: 1200px; height: auto; margin: 0 auto;}
.regi_title { height: 70px; font-size:20px; line-height: 50px;}
.regi_center, .all_content_de { width: 1200px; height: auto; margin: 0 auto; border: 1px solid #cfd1c2;}
.header_content { width: 1180px; height: 320px; border: 1px solid #cfd1c2; margin: 8px;}
.material_content { width: 1180px; height: auto; border: 1px solid #cfd1c2; margin: 8px; overflow: hidden;}
.cate_line{ width: 800px; margin: 20px; height: 30px; }
.cate_line:nth-child(3){ height: 110px;}
#why { width: 1100px; height: 120px;} 
#wht p { height:250px;}
.cate1{ width: 120px; height: 30px; line-height: 30px; float: left; margin: 0;}
.form-control, select { float: left;}
.regi_center select { width: 100px; height: 25px;}
.imagepath {float: right;}
#mainPhotoHolder{ margin: 30px 60px;}

.material_detail{ width: 450px; height: 50px; float: left;}
.material_detail:first-child{ margin-left: 160px;}
.material_detail input { width:110px; height: 30px; }
.btn-mat-add {width: 50px; height: 50px; margin-bottom: 10px;}

.recipe-detail{ width: 800px; height: 120px; margin: 0 auto;}
.rec_text textarea { border-radius: 10px; margin-right: 1px;}
.seq_content{ width: 1180px; height: auto; border: 1px solid #cfd1c2; margin: 8px;}
#seq_img, .rec_pic  { width:120px; height: 105px; border: 1px solid #cfd1c2; border-radius: 10px; cursor:pointer; float: left;}
.regi_btn{ height: 150px; margin-top: 30px;}

.detail_header { width: 1180px; height: auto; border: 1px solid #cfd1c2; margin: 8px;}
.imagepath-de { height: 380px; }
.detail_writer { width :150px; height: 50px; margin: 0 auto; }
.detail_dq1, .detail_dq2 { width: 30px; height: 30px; margin: 0; padding: 0 10px;}
.subtitle_line { width : 700px; height: auto; margin: 30px auto; color: #7c7c7d;}
.detail_line1 { width: 445px; height: 100px; margin: 20px auto;}
.detail_line1 img { width: 50px; height: 50px;}
.detail_line2 { padding: 20px 45px;}



/* Ranking Folder */
.list_lump { width:99%;}
.chef_list4_in {background:#fff; padding:2px;}
.chef_list4_in .list_lump {padding:18px 5px;}
.chef_list4_in .list_mem3 {display:inline-block; vertical-align:middle; padding-right:10px;}
.chef_list4_in .list_mem3 .mem_pic {display:inline-block; vertical-align:top;}
.chef_list4_in .list_mem3 .mem_pic img {width:70px; height:70px; border-radius:50%;}
.chef_list4_in .list_cont4 {display:inline-block; vertical-align:middle;}
.chef_list4_in .list_cont4 b {display:block; padding-bottom:6px;}
.chef_list4_in .list_cont4 b a {color:#de4830; font-size:18px;}
.chef_list4_in .list_cont4 .btn-sm {font-size:11px; padding:4px 8px; margin:-2px 0 1px 6px;}
.list_ranking2 {width:170px; line-height:1; display:inline-block; vertical-align:middle;}
.list_ranking2_num { width:90px; text-align:right; font-size:32px; color:#000; font-weight:900; margin:0 0 4px 0; display:inline-block; letter-spacing:-0.05em;}
.list_ranking2_num.txt4 {font-size:14px;}
.list_ranking2_num2 {font-size:13px; margin:0 0 2px 10px; display:inline-block; vertical-align:super;}
.list_ranking2_num2 span {padding-right:4px; vertical-align:text-top;}


.mem_cont {display:inline-block; padding:0 0 0 15px; line-height:18px;}
.mem_cont.st2 {width:720px;}
.mem_cont.st3 {width:300px;}
.mem_cont p {margin:0; padding:15px 0 0 0;}
.mem_cont b {display:block; font-size:16px; color:#000; padding:8px 0 6px 0;}
.mem_cont1 {background:url(http://112.164.58.12/hanbol/img/icon_rank/icon_fork.png) left top no-repeat; background-size:20px; padding-left:22px; font-size:14px; color:#888; margin-right:15px;}
.mem_cont2 {background:url(http://112.164.58.12/hanbol/img/icon_rank/icon_mem.png) left -3px no-repeat; background-size:20px; padding-left:21px; font-size:14px; color:#888; margin-right:15px;}
.mem_cont3 {background:url(http://112.164.58.12/hanbol/img/icon_rank/icon_scrap.png) left -1px no-repeat; background-size:20px; padding:0 0 2px 21px; font-size:14px; color:#888; margin-right:15px;}
.mem_cont7 {background:url(http://112.164.58.12/hanbol/img/icon_rank/icon_view3.png) left -1px no-repeat; background-size:19px; padding-left:21px; font-size:14px; color:#888; margin-right:15px;}
.glyphicon-comment:before {content:"hi"}

.box {
    width: 500px;
    height: 150px;
    float: left;
    list-style: none;
    background: #cccccc;
    margin-left: 1px;
    line-height: 150px;
}
.my_tabs a {
	text-decoration: none;
	display:block;
	width:100%;
	height: 100%;
	color: #3d3d3d;
	font-size: 30px;
	font-weight: bold;
}

.box:hover {
	background: #d6d6d6;
}

.active {
	background: #6e6e6e;
}
.active .act {
	color:#fff;
}

.cont_list { margin-top:14px;}

.cont_list a {
	font-size:12px;
}

.rec-list {
	float:left;
	margin-left: 15px;
	margin-bottom : 10px;
	list-style: none;
	border : 3px solid #ccc;
}
.rec-list:hover {
	border: 3px solid #5ca920;
    text-decoration: none;
    cursor: pointer;
}


#content { overflow: hidden; width: 1122px; height: auto; margin: 0 auto; box-sizing: border-box; margin-top: 10px;}
	
#section { float: right;width: 955px; height: auto; padding: 10px; border: 1px solid #cccccc; box-sizing: border-box; border-radius: 5px;}

#aside { width: 160px; height: auto; float: left; border: 1px solid #ccc;  border-radius: 5px;}

#aside_menu { display: block; padding: 0; margin: 0; background-color: #999966; }

#aside ul li { overflow: hidden; border: 1px solid #cccccc; padding: 10px; }

.item a { font-size: 18px; font-family: 'Poor Story', cursive; font-weight: bold;}

.item:hover { color: white; }

.title { height: 80px; line-height: 50px; margin-bottom: 10px; text-align: center; }


































